#include <iostream>

using namespace std;

int main()
{
    int n,i,d,nr=0;
    cin>>n;
    for(i=1;nr<=n;i++)
    {
        for(d=1;d<=i;d++)
        {
            cout<<d<<" ";
            nr++;
            if(nr==n)
            {
                break;
            }
        }
        if(nr==n)
        {
            break;
        }
    }
    return 0;
}
